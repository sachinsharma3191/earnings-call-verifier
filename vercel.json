{
  "buildCommand": "npm run build",
  "outputDirectory": "ui/dist",
  "installCommand": "npm install && npm --prefix ui install",
  "git": {
    "deploymentEnabled": false
  },
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api" },
    { "source": "/((?!api).*)", "destination": "/index.html" }
  ]
}
