{
  "name": "earnings-call-verifier",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "install": "npm --prefix ui install",
    "dev": "npm --prefix ui run dev",
    "build": "npm run build-cache && npm run sync:api && npm --prefix ui run build",
    "build-cache": "node scripts/build-cache.js",
    "sync:api": "node scripts/sync-api.mjs",
    "prefetch": "node scripts/prefetch-on-start.js",
    "dev:with-cache": "npm run dev & sleep 3 && npm run prefetch",
    "api": "node bin/server.js",
    "worker": "node bin/worker.js",
    "worker:watch": "node bin/worker.js --watch"
  },
  "devDependencies": {
    "vite": "^7.3.1"
  },
  "dependencies": {
    "@fastify/swagger": "^9.7.0",
    "axios": "^1.13.5",
    "cheerio": "^1.2.0",
    "fastify": "^5.7.4"
  }
}